{
  "name": "treelsp-monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "pnpm -r --filter '!examples' build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "dev": "pnpm -r dev",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "build:examples": "cd packages/examples/mini-lang && node ../../cli/dist/index.js build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.5",
    "eslint": "^9.39.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^2.1.8"
  }
}
